,IAAI,CAACzC,SAAnB,EAA6B;AAC5ByC,gBAAAA,IAAI,CAACT,OAAL;AACA;AACA;;AACD,kBAAGS,IAAI,CAACxC,QAAL,CAAcoB,IAAd,IAAsB,IAAzB,EAA+B;AAC9B,oBAAIZ,KAAS,GAAGgC,IAAI,CAACxC,QAAL,CAAcQ,KAA9B;AACA,oBAAIoC,KAAY,GAAG,CAACJ,IAAI,CAACxC,QAAL,CAAcgB,QAAd,IAA0BwB,IAAI,CAACzC,SAAL,GAAiB4C,OAA3C,CAAD,IAAwDH,IAAI,CAACxC,QAAL,CAAcgB,QAAzF;AACA4B,gBAAAA,KAAK,GAAGC,IAAI,CAACC,GAAL,CAASF,KAAT,EAAe,CAAf,CAAR;;AACA,oBAAGJ,IAAI,CAACxC,QAAL,CAAciB,IAAjB,EAAuB;AACtB2B,kBAAAA,KAAK,GAAGJ,IAAI,CAACxC,QAAL,CAAciB,IAAd,CAAmB2B,KAAnB,EAAyB,CAAzB,EAA2B,CAA3B,EAA6B,CAA7B,CAAR;AACA;;AACD,qBAAI,IAAIG,OAAR,IAAmBvC,KAAnB,EAA0B;AACzB,sBAAGgC,IAAI,CAAC3C,UAAL,CAAgBkD,OAAhB,KAA4BC,SAA/B,EAA0C;AACzCR,oBAAAA,IAAI,CAAC3C,UAAL,CAAgBkD,OAAhB,IAA2BP,IAAI,CAAC9C,OAAL,CAAaqD,OAAb,CAA3B;AACA;;AACD,sBAAIE,IAAW,GAAGzC,KAAK,CAACuC,OAAD,CAAL,GAAiBP,IAAI,CAAC3C,UAAL,CAAgBkD,OAAhB,CAAnC;AACAP,kBAAAA,IAAI,CAAC9C,OAAL,CAAaqD,OAAb,IAAwBP,IAAI,CAAC3C,UAAL,CAAgBkD,OAAhB,IAA4BE,IAAI,GAAGL,KAA3D,CALyB,CAMzB;AACA;AACA;AACA;AACA;AACD;;AACD,kBAAGJ,IAAI,CAACpC,SAAL,IAAkBoC,IAAI,CAACnC,YAA1B,EAAwC;AACvCmC,gBAAAA,IAAI,CAACpC,SAAL,CAAe+B,KAAf,CAAqBK,IAAI,CAACnC,YAA1B;AACA;AACD,aA5BgB,EA4Bf,CA5Be,CAAjB;AA6BA;AACD;;AAEO6B,QAAAA,UAAU,GAAQ;AACzB,cAAG,CAAC,KAAKpC,QAAT,EAAmB;;AACnB,cAAG,KAAKJ,OAAL,CAAawD,YAAb,GAA4B,CAA/B,EAAkC;AACjC,iBAAKxD,OAAL,CAAawD,YAAb;AACA;;AACD,cAAIC,KAAY,GAAG3D,YAAY,CAAC4D,SAAb,CAAuBC,OAAvB,CAA+B,IAA/B,CAAnB;;AACA,cAAGF,KAAK,IAAI,CAAC,CAAb,EAAgB;AACf3D,YAAAA,YAAY,CAAC4D,SAAb,CAAuBE,MAAvB,CAA8BH,KAA9B,EAAoC,CAApC;AACA;;AACD,eAAKI,KAAL;AACA;;AAEMA,QAAAA,KAAK,GAAQ;AACnB,cAAG,CAAC,KAAKzD,QAAT,EAAmB;;AACnB,cAAG,KAAKQ,SAAL,IAAkB,CAAC,CAAtB,EAAyB;AACxB8B,YAAAA,MAAM,CAACK,KAAP,CAAae,aAAb,CAA2B,KAAKlD,SAAhC;AACA,iBAAKA,SAAL,GAAiB,CAAC,CAAlB;AACA;;AACD,eAAKH,QAAL,GAAgB,CAAhB;AACA,eAAKD,KAAL,GAAa,KAAb;AACA,eAAKG,YAAL,GAAoB,IAApB;AACA,eAAKD,SAAL,GAAiB,IAAjB;AACA,eAAKR,aAAL,GAAqB,EAArB;AACA,eAAKF,OAAL,GAAe,IAAf;AACA,eAAKC,MAAL,GAAc,IAAd;AACA,eAAKK,QAAL,GAAgB,IAAhB;AACA,eAAKC,SAAL,GAAiB,IAAjB;AACA,eAAKH,QAAL,GAAgB,KAAhB;;AACA,cAAGN,YAAY,CAACiE,SAAb,CAAuBJ,OAAvB,CAA+B,IAA/B,KAAwC,CAAC,CAA5C,EAA+C;AAC9C7D,YAAAA,YAAY,CAACiE,SAAb,CAAuB5B,IAAvB,CAA4B,IAA5B;AACA;AACD;;AAxLW,O;;yBA2LQrC,Y,GAAN,MAAMA,YAAN,CAAmB;AAEP,eAARkE,QAAQ,GAAU;AAC7B,iBAAOlE,YAAY,CAACiE,SAAb,CAAuBxB,KAAvB,MAAkC,IAAI1C,MAAJ,EAAzC;AACH;;AAGqB,eAARoE,QAAQ,CAACC,MAAD,EAAYpD,KAAZ,EAA+B;AACjD,cAAIqD,EAAS,GAAGrE,YAAY,CAACkE,QAAb,EAAhB;AACAG,UAAAA,EAAE,CAACjD,SAAH,CAAagD,MAAb;AACAC,UAAAA,EAAE,CAACtD,QAAH,CAAYC,KAAZ;;AACA,cAAG,CAACoD,MAAM,CAACV,YAAX,EAAyB;AACrBU,YAAAA,MAAM,CAACV,YAAP,GAAsB,CAAtB;AACH;;AACDU,UAAAA,MAAM,CAACV,YAAP;AACA1D,UAAAA,YAAY,CAAC4D,SAAb,CAAuBvB,IAAvB,CAA4BgC,EAA5B,EARiD,CASjD;;AACA,iBAAOA,EAAP;AACH;;AAEyB,eAAZC,YAAY,CAACF,MAAD,EAAkB;AACxC,cAAG,CAACA,MAAM,CAACV,YAAR,IAAwBU,MAAM,CAACV,YAAP,IAAuB,CAAlD,EAAqD;AACrD,cAAIa,GAAY,GAAGvE,YAAY,CAAC4D,SAAhC;;AACA,eAAI,IAAIY,CAAQ,GAAGD,GAAG,CAAC/B,MAAJ,GAAa,CAAhC,EAAmCgC,CAAC,IAAI,CAAxC,EAA2CA,CAAC,EAA5C,EAAgD;AAC5C,gBAAGD,GAAG,CAACC,CAAD,CAAH,CAAOlD,SAAP,MAAsB8C,MAAzB,EAAiC;AAC7BG,cAAAA,GAAG,CAACC,CAAD,CAAH,CAAOT,KAAP;AACAQ,cAAAA,GAAG,CAACT,MAAJ,CAAWU,CAAX,EAAa,CAAb;AACH;AACJ;;AACDJ,UAAAA,MAAM,CAACV,YAAP,GAAsB,CAAtB;AACH;;AA9B6B,O;;AAAb1D,MAAAA,Y,CACHiE,S,GAAqB,E;AADlBjE,MAAAA,Y,CAMH4D,S,GAAqB,E","sourcesContent":["/**\r\n * 对象池管理缓动\r\n * @author Liz 2019年4月18日21:10:57\r\n */\r\nclass GTween {\r\n\tprivate _target:any;\r\n\tprivate _steps:any[];\r\n\tprivate _initialProps:any;\r\n\tprivate _stepProps:any;\r\n\tprivate _isStart:boolean = false;\r\n\tprivate _stepTime:number;\r\n\tprivate _curStep:any;\r\n\tprivate _lastStep:any;\r\n\r\n\tprivate _loop:boolean = false;\r\n\tprivate _stepIdx:number = 0;\r\n\tprivate _onChange:Function;\r\n\tprivate _onChangeObj:any;\r\n\tpublic constructor() {\r\n\t}\r\n\r\n\tpublic setProps(props:any):void {\r\n\t\tif(!props) return;\r\n\t\tthis._loop = props.loop == true;\r\n\t\tthis._onChange = props.onChange;\r\n\t\tthis._onChangeObj = props.onChangeObj;\r\n\t}\r\n\r\n\tpublic setTarget(t:any) {\r\n\t\tthis._target = t;\r\n\t\tthis._initialProps = {};\r\n\t}\r\n\r\n\tpublic getTarget():any {\r\n\t\treturn this._target;\r\n\t}\r\n\r\n\tpublic to(props:any,duration?:number,ease?: Function):GTween {\r\n\t\tif(!this._target) return;\r\n\t\tif(!props || typeof(props) != \"object\") return;\r\n\t\tif(!duration || duration < 0) {\r\n\t\t\treturn;\r\n\t\t}\r\n\t\tfor(let key in props) {\r\n\t\t\tthis._initialProps[key] = props[key];\r\n\t\t}\r\n\t\tthis.addStep({\r\n\t\t\ttype:\"to\",\r\n\t\t\tprops:props,\r\n\t\t\tduration:duration,\r\n\t\t\tease:ease});\r\n\t\treturn this;\r\n\t}\r\n\r\n\tpublic call(callback: Function, thisObj?: any, params?: any[]):GTween {\r\n\t\tthis.addStep({\r\n\t\t\ttype : \"call\",\r\n\t\t\tcallBack : callback,\r\n\t\t\tthisObj : thisObj,\r\n\t\t\tparams : params});\r\n\t\treturn this;\r\n\t}\r\n\r\n\tpublic wait(time:number):GTween {\r\n\t\tthis.addStep({\r\n\t\t\ttype : \"wait\",\r\n\t\t\tduration : time});\r\n\t\treturn this;\r\n\t}\r\n\r\n\tprivate addStep(step:any):void {\r\n\t\tif(!this._steps) {\r\n\t\t\tthis._steps = [];\r\n\t\t}\r\n\t\tthis._steps.push(step);\r\n\t\tthis.beginTween();\r\n\t}\r\n\t\r\n\tprivate beginTween():void {\r\n\t\tif(this._isStart) return;\r\n\t\tthis._isStart = true;\r\n\t\tthis.getStep();\r\n\t}\r\n\r\n\tprivate getStep():void {\r\n\t\tif(!this._isStart) return;\r\n\t\tif(this._loop) {\r\n\t\t\tif(this._stepIdx >= this._steps.length) {\r\n\t\t\t\tthis._stepIdx = 0;\r\n\t\t\t\tfor(let key in this._initialProps) {\r\n\t\t\t\t\tthis._target[key] = this._initialProps[key];\r\n\t\t\t\t}\r\n\t\t\t}\r\n\t\t\tthis._curStep = this._steps[this._stepIdx];\r\n\t\t\tthis._stepIdx ++;\r\n\t\t}\r\n\t\telse {\r\n\t\t\tthis._curStep = this._steps.shift();\r\n\t\t}\r\n\t\t//属性校正\r\n\t\tif(this._lastStep && this._lastStep.props) {\r\n\t\t\tfor(let key in this._lastStep.props) {\r\n\t\t\t\tthis._target[key] = this._lastStep.props[key];\r\n\t\t\t}\r\n\t\t}\r\n\t\tthis._lastStep = this._curStep;\r\n\t\tif(!this._curStep) {\r\n\t\t\tthis.stopAction();\r\n\t\t\treturn;\r\n\t\t}\r\n\t\tif(this._curStep.type == \"call\") {\r\n\t\t\tthis._curStep.callBack.apply(this._curStep.thisObj,this._curStep.params);\r\n\t\t\tthis.getStep();\r\n\t\t\treturn;\r\n        }\r\n        \r\n\t\tthis._stepTime = engine.game.gameTime + this._curStep.duration;\r\n\t\tthis._stepProps = {};\r\n\t\tthis.runAction();\r\n\t}\r\n\r\n\tprivate timeIndex:number = -1;\r\n\tprivate runAction():void {\r\n\t\tif(this.timeIndex == -1) {\r\n            let self = this;\r\n\t\t\tthis.timeIndex = engine.timer.setInterval(function(){\r\n\t\t\t\tlet curTime:number = engine.game.gameTime;\r\n\t\t\t\tif(curTime >= self._stepTime){\r\n\t\t\t\t\tself.getStep();\r\n\t\t\t\t\treturn;\r\n\t\t\t\t}\r\n\t\t\t\tif(self._curStep.type == \"to\") {\r\n\t\t\t\t\tlet props:any = self._curStep.props;\r\n\t\t\t\t\tlet ratio:number = (self._curStep.duration - (self._stepTime - curTime)) / self._curStep.duration;\r\n\t\t\t\t\tratio = Math.min(ratio,1);\r\n\t\t\t\t\tif(self._curStep.ease) {\r\n\t\t\t\t\t\tratio = self._curStep.ease(ratio,0,1,1);\r\n\t\t\t\t\t}\r\n\t\t\t\t\tfor(let keyName in props) {\r\n\t\t\t\t\t\tif(self._stepProps[keyName] == undefined) {\r\n\t\t\t\t\t\t\tself._stepProps[keyName] = self._target[keyName];\r\n\t\t\t\t\t\t}\r\n\t\t\t\t\t\tlet diff:number = props[keyName] - self._stepProps[keyName];\r\n\t\t\t\t\t\tself._target[keyName] = self._stepProps[keyName] + (diff * ratio);\r\n\t\t\t\t\t\t// if(keyName == \"scaleX\" || keyName == \"scaleY\") {\r\n\t\t\t\t\t\t// \tconsole.log(keyName + \"-->dis : \" + dis,\"ratio : \" + ratio,\"step初始：\",this._stepProps[keyName],\"当前：\",this._target[keyName]);\r\n\t\t\t\t\t\t// }\r\n\t\t\t\t\t\t// console.log(keyName + \"-->dis : \" + dis,\"ratio : \" + ratio);\r\n\t\t\t\t\t}\r\n\t\t\t\t}\r\n\t\t\t\tif(self._onChange && self._onChangeObj) {\r\n\t\t\t\t\tself._onChange.apply(self._onChangeObj);\r\n\t\t\t\t}\r\n\t\t\t},1);\r\n\t\t}\r\n\t}\r\n\r\n\tprivate stopAction():void {\r\n\t\tif(!this._isStart) return;\r\n\t\tif(this._target.gtween_count > 0) {\r\n\t\t\tthis._target.gtween_count--;\r\n\t\t}\r\n\t\tlet twIdx:number = TweenManager.TweenList.indexOf(this);\r\n\t\tif(twIdx != -1) {\r\n\t\t\tTweenManager.TweenList.splice(twIdx,1);\r\n\t\t}\r\n\t\tthis.reset();\r\n\t}\r\n\r\n\tpublic reset():void {\r\n\t\tif(!this._isStart) return;\r\n\t\tif(this.timeIndex != -1) {\r\n\t\t\tengine.timer.clearInterval(this.timeIndex);\r\n\t\t\tthis.timeIndex = -1;\r\n\t\t}\r\n\t\tthis._stepIdx = 0;\r\n\t\tthis._loop = false;\r\n\t\tthis._onChangeObj = null;\r\n\t\tthis._onChange = null;\r\n\t\tthis._initialProps = {};\r\n\t\tthis._target = null;\r\n\t\tthis._steps = null;\r\n\t\tthis._curStep = null;\r\n\t\tthis._lastStep = null;\r\n\t\tthis._isStart = false;\r\n\t\tif(TweenManager.TweenPool.indexOf(this) == -1) {\r\n\t\t\tTweenManager.TweenPool.push(this);\r\n\t\t}\r\n\t}\r\n}\r\n\r\nexport default class TweenManager {\r\n    public static TweenPool:GTween[] = [];\r\n    private static getTween():GTween {\r\n        return TweenManager.TweenPool.shift() || new GTween();\r\n    }\r\n\r\n    public static TweenList:GTween[] = [];\r\n    public static addTween(target:any,props?:any):GTween {\r\n        let tw:GTween = TweenManager.getTween();\r\n        tw.setTarget(target);\r\n        tw.setProps(props);\r\n        if(!target.gtween_count) {\r\n            target.gtween_count = 0;\r\n        }\r\n        target.gtween_count++;\r\n        TweenManager.TweenList.push(tw);\r\n        // console.log(\"当前缓动数量：\",TweenManager.TweenList.length,\"缓动池数量：\",TweenManager.TweenPool.length);\r\n        return tw;\r\n    }\r\n\r\n    public static removeTweens(target:any):void {\r\n        if(!target.gtween_count || target.gtween_count == 0) return;\r\n        let tws:GTween[] = TweenManager.TweenList;\r\n        for(let i:number = tws.length - 1; i >= 0; i--) {\r\n            if(tws[i].getTarget() == target) {\r\n                tws[i].reset();\r\n                tws.splice(i,1);\r\n            }\r\n        }\r\n        target.gtween_count = 0;\r\n    }\r\n}"]}